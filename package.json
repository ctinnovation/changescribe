{
  "name": "changescribe",
  "version": "1.1.1",
  "description": "",
  "main": "index.js",
  "keywords": [
    "markdown",
    "changelog",
    "changelog.md",
    "changelogger",
    "project",
    "management",
    "versioning",
    "semver",
    "devops",
    "tool",
    "changelog-generator",
    "code"
  ],
  "contributors": [
    "giovanni-bertoncelli",
    "elia-piacentini"
  ],
  "scripts": {
    "start": "node . --targetVersion 1.0.0",
    "test": "exit 0",
    "version": "git branch -D 'release_'$npm_package_version || true && git checkout -b 'release_'$npm_package_version && node ./cli.js -p && git add .",
    "postversion": "git commit -mv$npm_package_version && git push --set-upstream origin 'release_'$npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ctinnovation/changelogger.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "handlebars": "^4.7.7",
    "moment": "^2.29.1",
    "semver": "^7.3.5",
    "simple-git": "^2.40.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=16"
  }
}
